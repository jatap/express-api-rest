{
  "name": "XXX",
  "version": "1.0.0",
  "description": "XXX / Express API REST",
  "author": "Julio Antúnez Tarín <julio.antunez.tarin@gmail.com>",
  "license": "ISC",
  "main": "server/index.js",
  "keywords": [
    "javascript",
    "js",
    "node",
    "es6",
    "express",
    "api",
    "rest"
  ],
  "scripts": {
    "***": "<------------------ build -------------------------------------->",
    "build": "npm-run-all clean transpile",
    "start": "yarn prod",
    "server": "node .compiled/index.js",
    "dev": "DEBUG=XXX:* npm-run-all -s build server",
    "dev:watch": "nodemon",
    "prod": "NODE_ENV=production DEBUG=XXX:* npm-run-all -s build server",
    "transpile": "babel ./server -d .compiled",
    "clean": "rimraf .compiled",
    "commit": "git cz",
    "release": "standard-version --no-verify -t ''",
    "generate:salt": "node server/scripts/generate-salt.js",
    "****": "<----------------- lint --------------------------------------->",
    "lint": "yarn lint:js",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint:fix": "yarn lint:js -- --fix",
    "lint:check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "*****": "<---------------- tests -------------------------------------->",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch --updateSnapshot || true",
    "test:coverage": "jest --coverage --updateSnapshot || true",
    "test:coverage:open": "open reports/coverage/index.html",
    "test:dev": "jest --watch --coverage --updateSnapshot || true",
    "******": "<---------------- tests ------------------------------------->",
    "docs": "apidoc -i server -o .docs",
    "docs:open": "open .docs/index.html"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bodymen": "^1.1.1",
    "compression": "^1.7.4",
    "config": "^3.3.3",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.16",
    "morgan": "~1.10.0",
    "npm-run-all": "^4.1.5",
    "querymen": "^2.1.4",
    "rimraf": "^3.0.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@babel/register": "^7.12.13",
    "@types/jest": "^26.0.20",
    "apidoc": "^0.26.0",
    "apidoc-markdown": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "express-dynamic-middleware": "^1.0.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.5.4",
    "mongodb-memory-server": "^6.9.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "supertest": "^6.1.3",
    "typescript": "^4.1.5"
  },
  "engines": {
    "node": "14.x.x",
    "npm": "7.x.x"
  }
}
